<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800" prefWidth="1600" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Tableview.KesuController">
   <children>
      <TableView fx:id="table" layoutX="20.0" layoutY="40.0" prefHeight="750" prefWidth="1500">
        <columns>
          <TableColumn fx:id="CharId" prefWidth="20.0" text="ID" />
          <TableColumn fx:id="Charname" prefWidth="110.0" text="Name" />
                  <TableColumn fx:id="CharInfo" prefWidth="1120.0" text="Info" />  
                  <TableColumn fx:id="CharURL" prefWidth="250.0" text="Link" /> 
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="8.0" mnemonicParsing="false" onAction="#GoBack" text="Back">
         <font>
            <Font name=".VnTeknicalH" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
